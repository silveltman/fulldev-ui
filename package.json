{
  "name": "fulldev-ui",
  "version": "0.5.16",
  "scripts": {
    "dev": "yarn tailwind:dev & yarn vite:dev",
    "build": "yarn tailwind:build && yarn vite:build",
    "vite:dev": "vite",
    "vite:build": "vite build",
    "tailwind:dev": "postcss ./src/lib/tailwind/main.css --dir ./src/lib -w",
    "tailwind:build": "postcss ./src/lib/tailwind/main.css --dir ./src/lib",
    "package": "svelte-kit sync && svelte-package && yarn tailwind:build"
  },
  "files": [
    "dist"
  ],
  "svelte": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./plugin": "./dist/plugin.cjs"
  },
  "types": "./dist/index.d.ts",
  "type": "module",
  "peerDependencies": {
    "svelte": "^3.54.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.18.0",
    "@sveltejs/package": "^2.0.2",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.0",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "svelte": "^3.59.1",
    "svelte-check": "^3.3.2",
    "tailwindcss": "^3.3.2",
    "tailwindcss-themer": "^3.1.0",
    "tslib": "^2.5.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.8"
  },
  "dependencies": {
    "@radix-ui/colors": "^0.1.8",
    "@rodneylab/svelte-social-icons": "^0.0.26",
    "@tailwindcss/forms": "^0.5.3",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-js": "^4.0.1"
  }
}
